def default_pods
    pod 'TSMarkdownParser', :path => '.'
end

target 'TSMarkdownParserTests' do
    default_pods
end

target 'TSMarkdownParserExample iOS' do
    #platform :ios
    default_pods
end

target 'TSMarkdownParserExample watchOS' do
    #platform :watchos
    default_pods
end

target 'TSMarkdownParserExample tvOS' do
    #platform :tvos
    default_pods
end

target 'TSMarkdownParserExample OSX' do
    #platform :osx
    default_pods
end
